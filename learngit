mkdir learn_git
cd learn_git
touch third.txt
git init
git add third.txt
git commit -m "adding third.txt"
git log
touch "fourth.txt"
git add fourth.txt
git commit -m "adding fourth.txt"
rm third.txt
git add
git commit -m "removing third.txt"
git log
git confing --global core.pager "cat"
git config --global --list
